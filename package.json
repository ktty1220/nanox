{
  "name": "nanox",
  "version": "0.2.0",
  "description": "Minimal framework for small projects using React",
  "keywords": [
    "framework",
    "react"
  ],
  "license": "MIT",
  "author": {
    "name": "ktty1220",
    "email": "ktty1220@gmail.com"
  },
  "main": "cjs/nanox.js",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktty1220/nanox.git"
  },
  "scripts": {
    "clean": "rimraf cjs umd",
    "build": "webpack && tsc",
    "dev": "webpack-dev-server",
    "prepare": "npm run build",
    "compile": "tsc",
    "exec-test": "jest || true",
    "exec-cov": "jest --coverage || true",
    "test": "run-s compile exec-test",
    "cov": "run-s compile exec-cov"
  },
  "dependencies": {
    "immutability-helper": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "ts-loader": "^6.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "verbose": false,
    "testEnvironment": "node",
    "testRegex": "/__tests__/.+\\.js?$"
  },
  "readmeFilename": "README.md"
}
