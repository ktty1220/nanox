{
  "name": "nanox",
  "version": "0.2.0",
  "description": "Minimal JavaScript framework for React inspired by Flux",
  "keywords": [
    "flux",
    "framework",
    "react"
  ],
  "license": "MIT",
  "author": {
    "name": "ktty1220",
    "email": "ktty1220@gmail.com"
  },
  "main": "dist/nanox.js",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktty1220/nanox.git"
  },
  "scripts": {
    "build": "webpack",
    "prepare": "npm run build",
    "cov": "jest --coverage",
    "test": "jest",
    "watch": "webpack -w"
  },
  "dependencies": {
    "eventemitter2": "^5.0.1",
    "react-micro-container": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "babel-jest": "^24.5.0",
    "jest": "^24.5.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.4000",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "verbose": false,
    "testEnvironment": "node",
    "testMatch": null,
    "testRegex": "/__tests__/.+\\.jsx?$",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "readmeFilename": "README.md"
}
