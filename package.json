{
  "name": "nanox",
  "version": "0.2.0",
  "description": "Minimal framework for small projects using React",
  "keywords": [
    "framework",
    "react"
  ],
  "license": "MIT",
  "author": {
    "name": "ktty1220",
    "email": "ktty1220@gmail.com"
  },
  "main": "dist/nanox.js",
  "typings": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktty1220/nanox.git"
  },
  "scripts": {
    "build": "webpack",
    "watch": "webpack -w",
    "dev": "webpack-dev-server",
    "prepare": "npm run build",
    "before-test": "tsc",
    "after-test": "rimraf src/nanox{.js.map,.js}",
    "exec-test": "jest || true",
    "exec-cov": "jest --coverage || true",
    "test": "run-s before-test exec-test after-test",
    "cov": "run-s before-test exec-cov after-test"
  },
  "dependencies": {
    "immutability-helper": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@types/node": "^11.13.0",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "babel-jest": "^24.7.1",
    "jest": "^24.7.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "rimraf": "^2.6.3",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "verbose": false,
    "testEnvironment": "node",
    "testRegex": "/__tests__/.+\\.js?$"
  },
  "readmeFilename": "README.md"
}
